{
  "name": "@agile-solid/components",
  "version": "1.0.0",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "dts": "tsc src/index.ts --declaration --emitDeclarationOnly --allowSyntheticDefaultImports --jsx preserve --jsxImportSource solid-js --skipLibCheck --target esnext --moduleResolution node --outDir dist",
    "build": "vite build && npm run dts"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huijiewei/agile-solid.git",
    "directory": "packages/components"
  },
  "peerDependencies": {
    "@agile-solid/twind": "*",
    "solid-js": ">=1",
    "twind": "next"
  },
  "dependencies": {
    "@agile-solid/hooks": "*",
    "@agile-solid/utils": "*"
  },
  "devDependencies": {
    "@agile-solid/test": "*"
  }
}
